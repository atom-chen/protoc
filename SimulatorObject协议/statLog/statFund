StatFund
statLogType	=			28
n1			=			byte	optType		// 操作类型
												1:购买基金	, 2:领取基金
n2			=			byte	type		// 基金类型
											1, "金币基金"
											2, "宝石基金" 
											3, "坐骑基金"
											4, "翅膀基金"
											5, "功勋基金"
											6, "强化基金"
											7, "经验基金"
						
n3			=			byte	day			// 领取那一天的基金(1-30)    0表示购买基金

n4			=			byte 	moneyType	// 购买基金货币类型	1金币2元宝 	
											领取为-1

n5			=			long	moneyNum	// 购买基金货币数量	